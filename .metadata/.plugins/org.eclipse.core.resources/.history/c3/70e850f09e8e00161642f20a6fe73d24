#THIS FILE CONTAINS VENDOR SPECIFIC PROPERTIES

jdbc.driver=com.mysql.jdbc.Driver
jdbc.url=JDBC:mysql://localhost:3306/test
jdbc.query.addEmployee=insert into employee values(?,?,?,?,?,?,?,?,?)
jdbc.query.modifyEmployee=update employee set name = ?, kinID =?, phoneNo= ?, dateOfBirth = ?, dateOfJoining = ?, address = ?, departmentID=?, projectID=?, rolesID=?
jadbc.query.removeEmployee= delete from employee where id = ?
jdbc.query.searchByID= select * from employee where id = ?
jdbc.query.searchByKinID= select * from employee where kinID = ?
jdbc.query.displayall = select * from employee